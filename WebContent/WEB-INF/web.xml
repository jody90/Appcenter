<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>sortimo</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>sortimo.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>error</servlet-name>
    <servlet-class>sortimo.controller.ErrorController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>error</servlet-name>
    <url-pattern>/error</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>sortimo.controller.IndexController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>formularmanager_index</servlet-name>
    <servlet-class>sortimo.formularmanager.controller.FormularManagerIndexController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>formularmanager_index</servlet-name>
    <url-pattern>/formularmanager/index</url-pattern>
    <url-pattern>/formularmanager/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>formularmanager_edit</servlet-name>
    <servlet-class>sortimo.formularmanager.controller.FormularManagerEditController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>formularmanager_edit</servlet-name>
    <url-pattern>/formularmanager/edit</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>formularmanager_list</servlet-name>
    <servlet-class>sortimo.formularmanager.controller.FormularManagerListController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>formularmanager_list</servlet-name>
    <url-pattern>/formularmanager/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>formularmanager_statistics</servlet-name>
    <servlet-class>sortimo.formularmanager.controller.FormularManagerStatisticsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>formularmanager_statistics</servlet-name>
    <url-pattern>/formularmanager/statistics</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>formularmanager_boss</servlet-name>
    <servlet-class>sortimo.formularmanager.controller.FormularManagerBossController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>formularmanager_boss</servlet-name>
    <url-pattern>/formularmanager/boss</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>formularmanager_todo</servlet-name>
    <servlet-class>sortimo.formularmanager.controller.FormularManagerTodoController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>formularmanager_todo</servlet-name>
    <url-pattern>/formularmanager/todo</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>formularmanager_public</servlet-name>
    <servlet-class>sortimo.formularmanager.controller.FormularManagerPublicController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>formularmanager_public</servlet-name>
    <url-pattern>/formularmanager/public</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>formularmanager_thanks</servlet-name>
    <servlet-class>sortimo.formularmanager.controller.FormularManagerThanksController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>formularmanager_thanks</servlet-name>
    <url-pattern>/formularmanager/thanks</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>settings_index</servlet-name>
    <servlet-class>sortimo.settings.controller.SettingsIndexController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>settings_index</servlet-name>
    <url-pattern>/settings/index</url-pattern>
    <url-pattern>/settings/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>/css/*</url-pattern>
    <url-pattern>/js/*</url-pattern>
    <url-pattern>/images/*</url-pattern>
    <url-pattern>/fonts/*</url-pattern>
  </servlet-mapping>
</web-app>